setup
  number i = 0
  repeat while(true)
    i = i+1
    if(isEven(i) and i > 10)
      break
    else if (isOdd(i))
      continue
    end if
  end while
end setup

functions
  boolean function isEven(number x)
    return true
  end function

  boolean function isOdd(number x)
    return true
  end function
end functions