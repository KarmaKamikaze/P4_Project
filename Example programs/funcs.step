variables
    constant number q = GetRandomNumber()
end variables

setup
end setup

loop
    number p = GetRandomNumber()
    Print(p)
    Wait(2000)
end loop

functions   
    number function GetRandomNumber()
        RandomSeed(420)
        return Random(0, 1000)
    end function
    
    blank function PrintRandomNumber()
        number x = GetRandomNumber()
        PrintNumber(x)
    end function
    
    blank function PrintNumber(number x)
        Print(x + q)
    end function
end functions