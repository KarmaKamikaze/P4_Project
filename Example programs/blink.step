# Based on the 'Blink' example for Arduino by Scott Fitzgerald, Arturo Guadalupi, and Colby Newman.
variables
  output digitalpin BuiltInLED = 13
end variables

setup
	Print("Hello boi")
end setup

loop
  WriteToDigitalPin(BuiltInLED, On)
  Wait(1000)
  WriteToDigitalPin(BuiltInLED, Off)
  Wait(1000)
end loop